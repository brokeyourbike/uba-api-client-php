{
    "name": "brokeyourbike/uba-api-client",
    "description": "United Bank for Africa API Client for PHP",
    "type": "library",
    "license": "MPL-2.0",
    "keywords": [
        "api",
        "api-client",
        "uba",
        "united-bank",
        "united-bank-for-africa"
    ],
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\UnitedBank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\UnitedBank\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "brokeyourbike/http-client": "^1.0",
        "brokeyourbike/resolve-uri": "^1.0",
        "brokeyourbike/http-enums": "^2.0",
        "brokeyourbike/has-source-model": "^2.0",
        "brokeyourbike/data-transfer-object": "^0.1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4"
    },
    "version": "0.2.2"
}
